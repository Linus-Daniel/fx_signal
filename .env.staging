# Staging Environment Variables
NODE_ENV=staging

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://staging-api.forexsignals.com/v1
EXPO_PUBLIC_API_TIMEOUT=12000

# App Configuration
EXPO_PUBLIC_APP_NAME=Forex Signals Pro (Staging)
EXPO_PUBLIC_APP_VERSION=1.0.0-staging

# External News API Keys (use staging keys)
EXPO_PUBLIC_NEWS_API_KEY=staging-newsapi-key
EXPO_PUBLIC_ALPHA_VANTAGE_KEY=staging-alphavantage-key
EXPO_PUBLIC_FINAGE_KEY=staging-finage-key
EXPO_PUBLIC_TRADING_ECONOMICS_KEY=staging-trading-economics-key

# RSS2JSON API Key
EXPO_PUBLIC_RSS2JSON_KEY=staging-rss2json-key

# Socket Configuration
EXPO_PUBLIC_SOCKET_URL=wss://staging-api.forexsignals.com/socket
EXPO_PUBLIC_SOCKET_RECONNECT_ATTEMPTS=5

# Notification Configuration
EXPO_PUBLIC_NOTIFICATION_ENABLED=true
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=staging_push_notification_key

# Analytics
EXPO_PUBLIC_ANALYTICS_API_KEY=staging_analytics_key

# Staging flags
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_ENABLE_LOGGING=true