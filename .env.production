# Production Environment Variables
NODE_ENV=production

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://prod-api.forexsignals.com/v1
EXPO_PUBLIC_API_TIMEOUT=15000

# App Configuration
EXPO_PUBLIC_APP_NAME=Forex Signals Pro
EXPO_PUBLIC_APP_VERSION=1.0.0

# External News API Keys (use production keys)
EXPO_PUBLIC_NEWS_API_KEY=prod-newsapi-key
EXPO_PUBLIC_ALPHA_VANTAGE_KEY=prod-alphavantage-key
EXPO_PUBLIC_FINAGE_KEY=prod-finage-key
EXPO_PUBLIC_TRADING_ECONOMICS_KEY=prod-trading-economics-key

# RSS2JSON API Key
EXPO_PUBLIC_RSS2JSON_KEY=prod-rss2json-key

# Socket Configuration
EXPO_PUBLIC_SOCKET_URL=wss://prod-api.forexsignals.com/socket
EXPO_PUBLIC_SOCKET_RECONNECT_ATTEMPTS=3

# Notification Configuration
EXPO_PUBLIC_NOTIFICATION_ENABLED=true
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=prod_push_notification_key

# Analytics
EXPO_PUBLIC_ANALYTICS_API_KEY=prod_analytics_key

# Production flags
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_ENABLE_LOGGING=false