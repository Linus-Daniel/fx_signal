# Environment Variables for Forex Signals Pro
# Development Configuration
NODE_ENV=development

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://api.example.com/v1
EXPO_PUBLIC_API_TIMEOUT=10000

# App Configuration
EXPO_PUBLIC_APP_NAME=Forex Signals Pro
EXPO_PUBLIC_APP_VERSION=1.0.0

# External News API Keys
EXPO_PUBLIC_NEWS_API_KEY=your-newsapi-key-from-newsapi.org
EXPO_PUBLIC_ALPHA_VANTAGE_KEY=your-alphavantage-key
EXPO_PUBLIC_FINAGE_KEY=your-finage-key
EXPO_PUBLIC_TRADING_ECONOMICS_KEY=your-trading-economics-key

# RSS2JSON API Key (optional, for higher limits)
EXPO_PUBLIC_RSS2JSON_KEY=your-rss2json-key

# Socket Configuration
EXPO_PUBLIC_SOCKET_URL=wss://api.example.com/socket
EXPO_PUBLIC_SOCKET_RECONNECT_ATTEMPTS=5

# Notification Configuration
EXPO_PUBLIC_NOTIFICATION_ENABLED=true
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your_push_notification_key_here

# Analytics
EXPO_PUBLIC_ANALYTICS_API_KEY=your_analytics_key_here

# Development flags
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_ENABLE_LOGGING=true
